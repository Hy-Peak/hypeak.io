{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["vue/types/vue"],
    "moduleResolution": "node",
    "allowImportingTsExtensions": false,
    "baseUrl": ".",
    "paths": {
      "@": ["src/*"]
    },
  },
  "include": ["src/**/*.ts", "src/**/*.vue","**/*.vue"],
  "exclude": [
    "node_modules"
  ]

}
